{
  "expo": {
    "name": "Employee Salary Management",
    "slug": "employee-salary-manager",
    "version": "1.0.0",
    "description": "Employee Salary Management App for managing employee salaries and expenses for a company or organization. It is a simple and easy to use app that allows you to manage your employees' salaries and expenses.",
    "orientation": "portrait",
    "icon": "./assets/images/all.jpg",
    "scheme": "employeesalarymanager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/all.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/all.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/all.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7dcd2d29-b6aa-494f-9863-79adb43439a1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7dcd2d29-b6aa-494f-9863-79adb43439a1"
    }
  }
}
